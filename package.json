{
  "name": "Glee",
  "version": "0.3.2",
  "description": "能不能做最好的网易云音乐播放器之一呢 😶",
  "main": "main.js",
  "author": {
    "name": "nondanee",
    "email": "iminezn5656@outlook.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nondanee/Glee.git"
  },
  "scripts": {
    "start": "./node_modules/.bin/electron .",
    "package-win": "./node_modules/.bin/electron-packager . --platform=win32 --arch=x64 --version=3.0.10 --icon=./resource/glee.ico --out=dist/ --overwrite=true --ignore=dist/ --ignore=screenshot/ --prune=true",
    "package-mac": "./node_modules/.bin/electron-packager . --platform=darwin --arch=all --version=3.0.10 --icon=./resource/glee.icns --out=dist/ --overwrite=true --ignore=dist/ --ignore=screenshot/ --prune=true",
    "package-linux": "./node_modules/.bin/electron-packager . --platform=linux --arch=x64 --version=3.0.10 --icon=./resource/glee.png --out=dist/ --overwrite=true --ignore=dist/ --ignore=screenshot/ --prune=true"
  },
  "build": {
    "appId": "com.nondanee.glee"
  },
  "devDependencies": {
    "electron-packager": "^9.0.0",
    "electron-rebuild": "^1.8.2"
  },
  "dependencies": {
    "big-integer": "^1.6.40",
    "electron": "^3.0.10",
    "node-id3": "^0.1.7",
    "windows-swca": "^1.1.1"
  }
}
